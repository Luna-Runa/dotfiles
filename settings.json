  // "remote.SSH.configFile": "/home/luna/.ssh/config",
  "terminal.integrated.scrollback": 5000,
  "editor.minimap.showSlider": "always",
  "editor.minimap.maxColumn": 20,
  "editor.minimap.size": "fill",
  "workbench.editorLargeFileConfirmation": 2048,
  "editor.largeFileOptimizations": false,
  "redhat.telemetry.enabled": true,
  "makefile.configureOnOpen": true,
  "workbench.editorAssociations": {
    "*.xlsx": "gc-excelviewer-excel-editor"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "diffEditor.ignoreTrimWhitespace": false
  },
  "[xml]": {
    "editor.defaultFormatter": "mikeburgh.xml-format"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "glassit.alpha": 255,
  "xml.server.vmargs": "-Xmx128M",
  "window.commandCenter": true,
  "editor.minimap.renderCharacters": false,
  "workbench.colorTheme": "Slack Theme Dark Mode",
  "docker.extension.enableComposeLanguageServer": true,
  "window.menuBarVisibility": "toggle",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "security.promptForLocalFileProtocolHandling": false,
  "security.promptForRemoteFileProtocolHandling": false,
  "database-client.autoSync": true
